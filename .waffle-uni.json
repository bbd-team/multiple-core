{
  "compilerVersion": "./node_modules/solc",
  "outputType": "all",
  "outputHumanReadableAbi": true,
  "sourceDirectory": "./contracts/external",
  "compilerOptions": {
    "outputSelection": {
      "*": {
        "*": [
          "evm.bytecode.object",
          "evm.deployedBytecode.object",
          "abi",
          "evm.bytecode.sourceMap",
          "evm.deployedBytecode.sourceMap",
          "metadata"
        ],
        "": [
          "ast"
        ]
      }
    },
    "evmVersion": "istanbul",
    "optimizer": {
      "enabled": true,
      "runs": 200
    }
  }
}